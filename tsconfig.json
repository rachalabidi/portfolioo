{
  "compilerOptions": {
    "jsx": "react-jsx",  // or "react" if you're using older React versions
    "allowJs": true,
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
